<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title - Fixed ERP</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link href="~/AdminLTE/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/AdminLTE/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/AdminLTE/bower_components/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/AdminLTE/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/AdminLTE/dist/css/skins/_all-skins.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <!-- jQuery 3 -->
    <script src="~/AdminLTE/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/AdminLTE/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- SlimScroll -->
    <script src="~/AdminLTE/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/AdminLTE/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/AdminLTE/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/AdminLTE/dist/js/demo.js"></script>

    <!-- Bootstrap DatePicker -->
    <link rel="stylesheet" href="~/AdminLTE/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Bootstrap DatePicker -->
    <script src="~/AdminLTE/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <script>
        $(function () {

            //Date picker
            $('#datepicker').datepicker({
                autoclose: true
            })
        })

    </script>
    <script src="~/Scripts/moment.min.js"></script>
    @RenderSection("scripts", required: false)
    @Html.DevExpress().GetStyleSheets(
            new StyleSheet { ExtensionSuite = ExtensionSuite.NavigationAndLayout },
            new StyleSheet { ExtensionSuite = ExtensionSuite.Editors },
            new StyleSheet { ExtensionSuite = ExtensionSuite.GridView }
        )
    @Html.DevExpress().GetScripts(
            new Script { ExtensionSuite = ExtensionSuite.NavigationAndLayout },
            new Script { ExtensionSuite = ExtensionSuite.Editors },
            new Script { ExtensionSuite = ExtensionSuite.GridView }
        )
</head>
<body class="hold-transition skin-blue sidebar-collapse sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index","Home",new { Area = ""})" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->">
                <span class="logo-mini"><b>FX</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><strong>Fixed</strong> ERP</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="hidden-xs">@Session["IdUsuario"]</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Search...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">Menú</li>
                    <li class="treeview active">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>General</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","Banco",new { area = "General"})"><i class="fa fa-circle-o"></i> Bancos</a></li>
                            <li><a href="@Url.Action("Index","Sucursal",new { area = "General"})"><i class="fa fa-circle-o"></i> Sucursales y Bodegas</a></li>
                            <li><a href="@Url.Action("Index","CatalogoTipo",new { area = "General"})"><i class="fa fa-circle-o"></i> Catálogos</a></li>
                            <li><a href="@Url.Action("Index","Empresa",new { area = "General"})"><i class="fa fa-circle-o"></i> Empresas</a></li>
                            <li><a href="@Url.Action("Index","Impuesto",new { area = "General"})"><i class="fa fa-circle-o"></i> Impuestos</a></li>
                            <li><a href="@Url.Action("Index","Pais",new { area = "General"})"><i class="fa fa-circle-o"></i> País</a></li>
                            <li><a href="@Url.Action("Index","Region",new { area = "General"})"><i class="fa fa-circle-o"></i> Región</a></li>
                            <li><a href="@Url.Action("Index","Provincia",new { area = "General"})"><i class="fa fa-circle-o"></i> Provincia</a></li>
                            <li><a href="@Url.Action("Index","Transportista",new { area = "General"})"><i class="fa fa-circle-o"></i> Transportistas</a></li>
                            <li><a href="@Url.Action("Index","Persona",new { area = "General"})"><i class="fa fa-circle-o"></i> Persona</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>Cuentas por pagar</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","ClaseProveedor",new { area = "CuentasPorPagar"})"><i class="fa fa-circle-o"></i> Clase proveedor</a></li>
                            <li><a href="@Url.Action("Index","TipoCodigoSRI",new { area = "CuentasPorPagar"})"><i class="fa fa-circle-o"></i> Código SRI</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>Seguridad y acceso</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","Usuario",new { area = "SeguridadAcceso"})"><i class="fa fa-circle-o"></i> Usuario</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>Contabilidad</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","AnioFiscal",new { area = "Contabilidad"})"><i class="fa fa-circle-o"></i> Año fiscal</a></li>
                            <li><a href="@Url.Action("Index","GrupoContable",new { area = "Contabilidad"})"><i class="fa fa-circle-o"></i> Grupos contables</a></li>
                            <li><a href="@Url.Action("Index","PeriodoContable",new { area = "Contabilidad"})"><i class="fa fa-circle-o"></i> Periodos</a></li>
                            <li><a href="@Url.Action("Index","TipoComprobante",new { area = "Contabilidad"})"><i class="fa fa-circle-o"></i> Tipo de comprobante</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>Roles</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","Division",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> División </a></li>
                            <li><a href="@Url.Action("Index","Area",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> Área </a></li>
                            <li><a href="@Url.Action("Index","Departamento",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> Departamento</a></li>
                            <li><a href="@Url.Action("Index","Cargo",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> Cargo</a></li>
                            <li><a href="@Url.Action("Index","Rubro",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> Rubro</a></li>
                            <li><a href="@Url.Action("Index","Contrato",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> Contrato</a></li>
                            <li><a href="@Url.Action("Index","NominaTipo",new { area = "RRHH"})"><i class="fa fa-circle-o"></i> Tipo de nómina</a></li>
                            <li><a href="@Url.Action("Index","TipoPrestamo",new { area = "RRHH"})"><i class="fa fa-circle-o"></i>Tipo préstamo</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>Inventario</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","ProductoTipo",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Tipo producto</a></li>
                            <li><a href="@Url.Action("Index","TipoMovimiento",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Tipo movimiento</a></li>
                            <li><a href="@Url.Action("Index","CatalogoTipoInventario",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Catalogo</a></li>
                            <li><a href="@Url.Action("Index","Categoria",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Categorización</a></li>
                            <li><a href="@Url.Action("Index","Marca",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Marca</a></li>
                            <li><a href="@Url.Action("Index","ProductoTipo",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Tipo producto</a></li>
                            <li><a href="@Url.Action("Index","ParametroInventario",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Parámetros</a></li>
                            <li><a href="@Url.Action("Index","UnidadDeMedida",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Unidad de medida</a></li>
                            <li><a href="@Url.Action("Index","Producto",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Producto</a></li>
                            <li><a href="@Url.Action("Index","MotivoInventario",new { area = "Inventario"})"><i class="fa fa-circle-o"></i> Motivos</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-folder-o"></i> <span>Facturación</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="@Url.Action("Index","ClienteTipo",new { area = "Facturacion"})"><i class="fa fa-circle-o"></i>Cliente</a></li>
                            <li><a href="@Url.Action("Index","TerminoPago",new { area = "Facturacion"})"><i class="fa fa-circle-o"></i>Termino pago</a></li>
                            <li><a href="@Url.Action("Index","TipoNota",new { area = "Facturacion"})"><i class="fa fa-circle-o"></i>Tipo Nota</a></li>
                            <li><a href="@Url.Action("Index","Vendedor",new { area = "Facturacion"})"><i class="fa fa-circle-o"></i>Vendedor</a></li>

                        </ul>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">@ViewBag.Title</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                <i class="fa fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        @RenderBody()
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer">
                        [@Session["IdEmpresa"]] @Session["nom_empresa"]
                    </div>
                    <!-- /.box-footer-->
                </div>
                <!-- /.box -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2018 Fixed S.A.</strong> All rights
            reserved.
        </footer>
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>


    <!-- ./wrapper -->

</body>
</html>
